<%= form_with model: @user, local: true, html: {multipart: true} do |f| %>

    <%= f.label :email, "Email Address" %>
    <br>
    <%= f.text_field :email %>
    <br><br>

    <%= f.label :password %>
    <br>
    <%= f.password_field :password %>
    <br><br>

    <%= f.label :password, "Confirm Password" %>
    <br>
    <%= f.password_field :password_confirmation %>
    <br><br>
 
    <%= f.label :name %>
    <br>
    <%= f.text_field :name %>
    <br><br>


    <%= f.submit %>

<% end %>